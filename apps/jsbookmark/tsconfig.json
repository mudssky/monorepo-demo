{
  "extends": ["@mudssky/tsconfig/jsbookmark.json"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist/tsoutput",
    "removeComments": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "rollup.config.ts"],
  "ts-node": {
    "esm": true
  }
}
