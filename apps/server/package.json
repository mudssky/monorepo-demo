    "@prisma/client": "5.8.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.3.2",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
