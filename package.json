    "@types/node": "^20.9.1",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "prettier-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.16"
  },
  "packageManager": "pnpm@8.6.10"
